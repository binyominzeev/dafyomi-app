<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dafkollel.dafyomi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DafyomiEnglish</name>
    <description>
        Contents of Dafyomi Kollel Point-By-Point outline.
    </description>
    <author email="szvbinjomin@gmail.com" href="http://github.com/binyominzeev">
        Binyomin Szanto-Varnagy
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.4.0" />
    <engine name="browser" spec="^5.0.4" />
</widget>
