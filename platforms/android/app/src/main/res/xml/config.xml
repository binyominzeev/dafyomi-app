<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dafkollel.dafyomi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>DafyomiEnglish</name>
    <description>
        Contents of Dafyomi Kollel Point-By-Point outline.
    </description>
    <author email="szvbinjomin@gmail.com" href="http://github.com/binyominzeev">
        Binyomin Szanto-Varnagy
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
